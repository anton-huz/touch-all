{
  "$schema": "../node_modules/oxfmt/configuration_schema.json",
  "insertFinalNewline": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
