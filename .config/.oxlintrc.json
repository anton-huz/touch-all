{
  "$schema": "../node_modules/oxlint/configuration_schema.json",
  "categories": {
    "correctness": "error"
  },
  "rules": {
    "eslint/no-unused-vars": "error"
  },
  "plugins": ["eslint", "typescript", "unicorn"],
  "overrides": [
    {
      "files": ["*.test.ts", "*.spec.ts"],
      "rules": {
        "@typescript-eslint/no-explicit-any": "off"
      },
      "plugins": ["jest"]
    }
  ]
}
