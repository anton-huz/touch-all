{
  "name": "create-fs",
  "version": "0.0.1",
  "description": "CLI tool to create folder structures from markdown tree representations",
  "keywords": [
    "cli",
    "folders",
    "generator",
    "markdown",
    "structure"
  ],
  "license": "MIT",
  "author": "",
  "bin": {
    "create-fs": "./dist/create-fs.js"
  },
  "type": "module",
  "main": "dist/create-fs.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm create-fs.ts",
    "check:lint": "oxlint . -c .config/oxlintrc.json",
    "check:format": "oxfmt . -c .config/oxfmtrc.json --check",
    "format": "oxfmt . -c .config/oxfmtrc.json --write"
  },
  "dependencies": {
    "@effect/cli": "^0.73.2",
    "@effect/platform-node": "^0.104.1",
    "effect": "^3.19.16"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "typescript": "^5.7.2"
  }
}
